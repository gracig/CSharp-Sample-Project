{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base",
        ":rebaseStalePrs",
        ":prImmediately",
        ":prHourlyLimitNone",
        ":prConcurrentLimitNone",
        ":automergeDisabled",
        ":disableDependencyDashboard"
    ],
    "baseBranches": [
        "main"
    ],
    "enabledManagers": [
        "nuget"
    ],
    "seperateMajorMinor": false

